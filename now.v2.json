{
  "version": 2,
  "name": "@standard-release/app",
  "alias": "standard-release",
  "builds": [
    { "src": "package.json", "use": "@now/node-server" }
  ],
  "env": {
    "APP_ID": "@tunnckocore-release-id",
    "NODE_ENV": "production",
    "PRIVATE_KEY": "@tunnckocore-release-private-key",
    "WEBHOOK_SECRET": "@tunnckocore-release-webhook-secret"
  }
}
